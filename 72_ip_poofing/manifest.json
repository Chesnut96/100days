{
  "name": "IP Poofing",
  "manifest_version": 2,
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": ["jquery.js", "content.js"],
    "run_at": "document_start"
  }],
  "permissions": [
    "<all_urls>",
    "webRequest",
    "tabs",
    "bookmarks"
  ]
}
