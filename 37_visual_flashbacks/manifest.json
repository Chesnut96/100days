{
  "name": "Visual Flashbacks",
  "manifest_version": 2,
  "version": "0.1",
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": ["jquery.js", "content.js"]
  }],
  "permissions": [
    "<all_urls>",
    "history",
    "tabs",
    "webNavigation"
  ]
}
