{
  "name": "High Five",
  "manifest_version": 2,
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
        "activeTab",
        "contextMenus",
    ],
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": ["jquery.js", "content.js"],
    "run_at": "document_start"
  }],
  "web_accessible_resources": ["popup/hand.html"]
}
